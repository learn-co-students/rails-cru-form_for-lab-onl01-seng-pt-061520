<h1>New Song</h1>
<%= form_for @song do |f| %>
  <%= f.label(:name, "Name: ") %>
  <%= f.text_field :name %><br>
  <%= f.label(:artist_id, "Artist Id: ") %>
  <%= f.text_field :artist_id %><br>
  <%= f.label(:genre_id, "Genre Id: ") %>
  <%= f.text_field :genre_id %><br>
  <%= f.submit %>
<% end %>
